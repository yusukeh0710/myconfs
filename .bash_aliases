alias gpp="g++-12 -std=gnu++20 -fsanitize=address -fsanitize=undefined -Wall -o ./out"
alias gppd="g++-12 -std=gnu++20 -fsanitize=address -fsanitize=undefined -Wall -D_GLIBCXX_DEBUG -o ./out"
